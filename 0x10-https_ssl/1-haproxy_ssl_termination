defaults
  mode http
  timeout client 15s
  timeout connect 10s
  timeout server 15s
  timeout http-request 10s
frontend www-https
    bind *:443 ssl crt /etc/letsencrypt/live/www.amirgambo.tech/fullchain.pem
    mode http
    default_backend www-backend

frontend clickviral-tech-frontend
    bind *:80
    default_backend clickviral-tech-backend
backend clickviral-tech-backend
    balance roundrobin
    server 137851-web-01 54.144.145.131:80 check
    server 137851-web-02 54.173.86.133:80 check
